            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        },
        {    "name": "启动爬虫",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/crawler.js",
            "stopOnEntry": false,
            "args": [],
